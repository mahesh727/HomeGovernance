<html>
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Script-Type" content="text/javascript">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile:Admin</title>
    <script src="https://unpkg.com/react@17/umd/react.development.js" crossorigin></script>
    <script src="https://unpkg.com/react-dom@17/umd/react-dom.development.js" crossorigin></script>
    <script src="https://unpkg.com/@babel/standalone/babel.min.js"></script>
    <script src="https://requirejs.org/docs/release/2.3.5/minified/require.js"></script>
    <style>
        <%- include("../public/App.css")%>
        </style>
  </head>
  <body>
    <div id="mydiv1223"></div>
    <script type="text/babel">
class Opening extends React.Component{
    Stylee=(ths)=>{
        for(var i=0;i<ths.length;i++)
        {if(ths[i].status)
          {
            return (<h1>{ths[i].job}</h1>
            );
          }
          }
        }
    render()
    {
      return(
        <div class="myDiv" style={{backgroundColor:"rgb(57, 61, 65)",color:'white'}}>
        <span>
            <div style={{width:500,backgroundColor:"rgb(57, 61, 65)"}}>
          <a href="/ProfileAdmin"> 
          <img style={imgsgs} alt="logo"/>
          <span class="title1" style={{float:'left',color:'lightblue'}}>Home governance</span>
          </a></div>
          <div style={{position:"relative", top:65}}>
        <span><input type="text" size="10" name="search" placeholder="search" class="searchs" align="centre"></input>
        <input type="submit" class="searchbutton" style={{width:80,}}></input></span></div>
          <div style={{position:"relative", left:200,width:100,backgroundColor:"rgb(57, 61, 65)"}}>
              <span>{this.Stylee(this.props.lists)}</span>
        </div>  
            <a href="/">
            <div style={{position:"relative", bottom:70,right:20,color:"white",backgroundColor:"rgb(57, 61, 65)", float:"right",}}>
                <h3>Logout</h3>
            </div>
            </a>
          </span>
        </div>
      );
    }
  }
  const imgsgs={
    float:'left',
    width:50,
  height:50,
  }
  class Side extends React.Component{
      Stylee=(k)=>{
          if(this.props.lists[k-1].status)
          {
            return {backgroundColor:"yellow", color:"black"};
          }
      }
      render()
      {
          return(
              <div class="myDiv" align="centre" style={{width:300,height:1000, backgroundColor:"rgb(57, 61, 65)", color:"white"}}>
                  <div style={{height:50}, this.Stylee(1)} onClick={this.props.clicker.bind(this,1)}>
                      <br/>
                      Dashboard
                  </div>
                  <div style={{height:50}, this.Stylee(2)} onClick={this.props.clicker.bind(this,2)}>
                  <br/>
                      Users
                  </div>
                  <div style={{height:50}, this.Stylee(3)}  onClick={this.props.clicker.bind(this,3)}>
                  <br/>
                      Professionals
                  </div>
                  <div style={{height:50}, this.Stylee(4)} onClick={this.props.clicker.bind(this,4)}>
                  <br/>
                      Reviews
                  </div>
              </div>
          );
      }
  }
// bottom:1000,left:500
//800
//1200
class Dash extends React.Component{
    render()
    {
        return(
            <h1>h4</h1>

        );
    }
}
class Users extends React.Component{
    render()
    {
        return(
            <h1>h5</h1>
        );
    }
}
class Professional extends React.Component{
    render()
    {
        return(
            <h1>h6</h1>
        );
    }
}
class Review extends React.Component{
    render()
    {
        return(
            <h1>h7</h1>
        );
    }
}
class Dis extends React.Component{
    render()
    {
        var j;
        for(var k=0;k<this.props.lists.length;k++)
        {if(this.props.lists[k].status)
        {if(k==0)
            {return(<Dash/>
        );}
        else if(k==1)
        {
            return(<Users/>
                );
        }
        else if(k==2)
        {
            return(<Professional/>
                );
        }
        else
        {
            return(<Review/>
                );
        }
        }
    }
}
}

class Main3 extends React.Component{
    state={
        i:1,
    lists:[
        {
            id:1,
            job:"Dashboard",
            status:false,
        },
        {
            id:2,
            job:"Users",
            status:true,
        },
        {
            id:3,
            job:"Professionals",
            status:false,
        },
        {
            id:4,
            job:"Reviews",
            status:false,
        },
    ]
    }
    afterclick=(j)=>
    {
        var k=0;
        while(k<this.state.lists.length)
        {
            if(this.state.lists[k].status==true & this.state.lists[k].id!=j )
            {
                this.state.lists[k].status=false;
                this.setState({lists:this.state.lists})
            }
            k++;
        }
        console.log(this.state.lists);
    }
    clicker=(j)=>
    {
        for(var k=0;k<this.state.lists.length;k++)
        {
            if(this.state.lists[k].id==j & this.state.lists[k].status==false)
            {
                this.state.lists[k].status=true;
                this.setState({lists:this.state.lists})
            }
        }
        this.afterclick(j);
    }
    render()
    {
        return(
            <div>
                <Opening lists={this.state.lists}/>
                <Side clicker={this.clicker} lists={this.state.lists}/>
                <Dis lists={this.state.lists}/>
            </div>
        );
    }
}
    ReactDOM.render(<Main3/>, document.getElementById('mydiv1223'))
    </script>
  </body>
</html>